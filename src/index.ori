=page.ori({
  title: "Cat Prints 🐾"
  @text: `
    <h1>Cat Prints 🐾</h1>
    <div class="grid">
    {{ @map _, (info, id) => `
      <a href="/prints/{{ id }}.html#" class="imageLink">
        <img src="/thumbnails/{{ id }}.jpg" alt="{{ info/title }}" class="print thumbnail" />
      </a>
    ` }}
    </div>
  `
})